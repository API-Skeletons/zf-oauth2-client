{
    "name": "zfcampus/zf-oauth2-client",
    "description": "An OAuth2 client for zf-oauth2",
    "license": "BSD-3-Clause",
    "keywords": [
        "api",
        "oauth2"
    ],
    "require": {
        "php": ">=5.3.23",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-uri": "~2.3",
        "zendframework/zend-servicemanager": "~2.3"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "zendframework/zend-test": "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-modulemanager": "~2.3",
        "zendframework/zend-config": "~2.3",
        "zendframework/zend-serializer": "~2.3",
        "zendframework/zend-log": "~2.3",
        "squizlabs/php_codesniffer": "~2",
        "satooshi/php-coveralls": "^0.6.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "ZF\\OAuth2\\Client\\": "src/",
            "ZFTest\\OAuth2\\Client\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
